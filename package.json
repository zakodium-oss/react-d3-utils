{
  "name": "react-d3-utils",
  "version": "0.1.0",
  "description": "Low-level utilities to build charts with React and D3",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-esm/index.d.ts",
  "files": [
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-ts",
    "build-ts": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "build-storybook": "build-storybook",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "dev": "start-storybook -p 6006",
    "eslint": "eslint --ext ts,tsx src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "npm run build",
    "start": "npm run dev",
    "test": "npm run eslint && npm run check-types",
    "storybook": "start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium/react-d3-utils.git"
  },
  "author": "Michael Zasso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium/react-d3-utils/issues"
  },
  "homepage": "https://github.com/zakodium/react-d3-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/addon-storysource": "^6.1.16",
    "@storybook/react": "^6.1.16",
    "@types/react": "^17.0.1",
    "@zakodium/eslint-config": "^2.2.2",
    "babel-loader": "^8.2.2",
    "eslint": "^7.19.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "use-resize-observer": "7.0.0"
  }
}
